{
  "name": "mrtuvn",
  "version": "1.0.0",
  "description": "Personal page about me, Contacts.",
  "main": "app.js",
  "scripts": {
    "preinstall": "npm i -g http-server phantomjs slimerjs casperjs backstopjs",
    "start": "webpack-dev-server",
    "development": "cp index.html app/index.html && NODE_ENV=development webpack && webpack-dev-server --content-base app/ --hot --inline --devtool inline-source-map --history-api-fallback",
    "build": "webpack && gulp build",
    "stats": "webpack --profile --json > stats.json",
    "eslint": "eslint . --ext .js --ext .jsx",
    "casper": "casperjs test tests/functional/",
    "backstop": "rm -rf tests/visual/backstop_data/bitmaps_test && backstop test --configPath=config/backstop.js --silent",
    "test": "npm run eslint && npm run casper --silent && npm run backstop"
  },
  "config": {
    "env": "develop"
  },
  "keywords": [
    "frontend"
  ],
  "author": "Tu Nguyen ladiesman9x@gmail.com https://mrtuvn.github.io",
  "license": "MIT",
  "homepage": "http://mrtuvn.github.io",
